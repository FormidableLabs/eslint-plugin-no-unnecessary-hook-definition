{
  "name": "eslint-plugin-no-unnecessary-hook-definition",
  "private": false,
  "version": "0.0.1",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": [
      {
        "import": "./dist/index.mjs",
        "default": "./dist/index.js"
      },
      "./dist/index.js"
    ],
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.7",
    "eslint": "^8.26.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "eslint": ">=7.0.0 || >=8.0.0"
  },
  "dependencies": {
    "eslint-traverse": "^1.0.0"
  },
  "scripts": {
    "test": "tsup ./src/no-unnecessary-hook-def.spec.ts --outDir .test --onSuccess 'node ./.test/no-unnecessary-hook-def.spec.js'",
    "test:watch": "yarn test --watch",
    "build": "tsup ./src/index.ts --outDir dist --format esm,cjs",
    "preversion": "yarn test",
    "prepack": "yarn build",
    "prepublishOnly": "yarn build"
  }
}